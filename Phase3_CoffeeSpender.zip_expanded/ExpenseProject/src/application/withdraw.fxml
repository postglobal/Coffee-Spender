<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" style="-fx-background-color: #C4A484;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <Label fx:id="withdrawLabel" layoutX="6.0" layoutY="14.0" text="How much would you like to subtract?" textFill="#141414">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button layoutX="120.0" layoutY="102.0" mnemonicParsing="false" onAction="#subtractMain" text="Subtract" />
      <TextField fx:id="amountToSubtract" layoutX="76.0" layoutY="63.0" promptText="Amount: $0.00" />
   </children>
</AnchorPane>
